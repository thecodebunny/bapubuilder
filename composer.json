{
    "name": "thecodebunny/bapubuilder",
    "description": "A drag and drop builder to manage pages, product in BAPU COMMERCE.",
    "keywords": [
        "bapubuilder"
    ],
    "homepage": "https://bapu-commerce.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hemang Vyas",
            "email": "info@bapu-commerce.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Thecodebunny\\Bapubuilder\\": "src/"
        },
        "files": [
            "src/Core/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Thecodebunny\\Bapubuilder\\BapubuilderServiceProvider"
            ],
            "aliases": {
                "Bapubuilder": "Thecodebunny\\Bapubuilder\\BapubuilderFacade"
            }
        }
    }
}
